package com.boco.domain.page;

import java.io.Serializable;

public class FixedCommandQueryPage implements Serializable {

	/**
	 * 
	 */
	private static final long serialVersionUID = 6500806277028744248L;
	/**
	 * 分页大小
	 */
	private Integer pageSize;
	/**
	 * 第几页
	 */
	private Integer pageIndex;
	/**
	 * @return the 分页大小
	 */
	public Integer getPageSize() {
		return pageSize;
	}
	/**
	 * @param 分页大小 the pageSize to set
	 */
	public void setPageSize(Integer pageSize) {
		this.pageSize = pageSize;
	}
	/**
	 * @return the 第几页
	 */
	public Integer getPageIndex() {
		return pageIndex;
	}
	/**
	 * @param 第几页 the pageIndex to set
	 */
	public void setPageIndex(Integer pageIndex) {
		this.pageIndex = pageIndex;
	}
	
	
}
