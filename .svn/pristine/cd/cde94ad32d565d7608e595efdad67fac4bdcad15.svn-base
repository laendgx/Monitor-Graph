@charset "utf-8";
html {
	width: 100%;
	height: 100%;
}
body {
	height: 100%;
}

cmwindow {
	position: relative;
	display: block;
	overflow: hidden;
	margin: 20px;
}

cmwindow .cmap,
cmwindow cmap {
	position: absolute;
	display: block;
	top: 0;
	left: 0;
}

cmwindow cmap {
	z-index: 11;
}

cmwindow cmap div {
	position: absolute;
	width: 32px;
	height: 32px;
	cursor: pointer;
}

cmwindow cmap img {
	position: absolute;
	/* width: 32px; */
	/* height: 32px; */
	transform: scale(0.5);
	cursor: pointer;
}

cmwindow eagle {
	display: block;
	position: fixed;
	border: 1px solid #000;
	box-shadow: 0 0 1px #fff;
	top: 20px;
	right: 20px;
	z-index: 21;
	overflow: hidden;
}

cmwindow eagle eawindow {
	display: block;
	position: absolute;
	border-color: rgba(0,0,0,.2);
	border-style: solid;
	/* border-width: 180px;
	top: -180px;
	left: -180px; */
	cursor: pointer;
}

cmwindow bgctrl {
	position: absolute;
	display: block;
	z-index: 41;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
}

cmscompile,
cmslist,
