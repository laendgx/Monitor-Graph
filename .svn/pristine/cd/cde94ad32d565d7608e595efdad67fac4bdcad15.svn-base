div.devices {
	position: absolute;
	height: 20px;
	bottom: 0;
	left: 0;
	z-index: 99;
}
div.devices div {
	float: left;
	height: 20px;
	width: 30px;
	border: 1px solid #fff;
	margin-left: 1px;
}

configbtn {
    position: absolute;
    display: block;
    width: 30px;
    height: 20px;
    background-color: #fff;
    color: #000;
    right: 0;
    bottom: 0;
    font-size: 12px;
    z-index: 201;
    text-align: center;
    line-height: 20px;
    cursor: pointer;
    transition: all 240ms ease;
}

configbtn:hover {
    background-color: #000;
    color: #fff;
    transition: all 240ms ease;
}

