@charset "utf-8";
body {
	position: relative;
	width: 100%;
	overflow: hidden;
}

polling {
	position: relative;
	display: block;
	margin: 20px;
	border: 1px solid #e0e0e0;
	box-sizing: border-box;
}

polling > header {
	position: relative;
	display: block;
	width: 100%;
	height: 40px;
	background-color: #fff;
	border-bottom: 1px solid #e0e0e0;
	box-sizing: border-box;
}

polling > header span {
	position: relative;
	display: block;
	line-height: 40px;
	color: #3db6ac;
	font-size: 20px;
	margin-left: 20px;
	letter-spacing: 2px;
}

polling polaside {
	position: relative;
	display: block;
	float: left;
	width: 268px;
	border-right: 1px solid #e0e0e0;
	box-sizing: border-box;
	background-color: #eff0f5;
}

polling polaside polscroll {
	position: absolute;
	display: block;
	width: 100%;
	left: 0;
}

polling polaside filtrate {
	position: relative;
	display: block;
	width: 100%;
	clear: both;
}

polling polaside filtrate.reset {
    width: 240px;
    margin: 0px auto;
    border-top: 1px solid #ccc;
}

polling polaside filtrate.reset filbtn {
    float: right;
}

polling polaside filtrate.reset filbtn span {
    cursor: pointer;
    transition: all 240ms ease;
}

polling polaside filtrate.reset filbtn span:hover {
    color: #C90509;
    box-shadow: 0 0 3px rgba(210,5,9,.6);
    border-color: rgba(210,5,9,.8);
    transition: all 240ms ease;
}

polling filtrate header {
	position: relative;
	width: 100%;
	height: 46px;
	line-height: 54px;
	color: #3e8dd8;
	font-size: 17px;
	text-indent: 20px;
}

polling filtrate filbtn {
	position: relative;
	width: 50%;
	display: block;
	float: left;
	height: 50px;
}

polling filtrate filbtn span {
	width: 104px;
    display: block;
    position: absolute;
    background: #fff;
    height: 30px;
    box-sizing: border-box;
    border: 1px solid #d1d9e3;
    border-radius: 6px;
    top: 10px;
    color: #8c9aae;
    left: 15px;
    font-size: 14px;
    line-height: 30px;
    text-align: center;
    overflow: hidden;
    transition: all 200ms ease;
    z-index: 2;
}

polling filtrate filbtn input:hover + span {
	box-shadow: 0 0 3px rgba(209,217,227,1);
	color: #535353;
    transition: all 200ms ease;
}

polling filtrate filbtn input {
	width: 104px;
    display: block;
    position: absolute;
    height: 30px;
    border-radius: 6px;
    top: 10px;
    left: 15px;
    opacity: 0;
    cursor: pointer;
    z-index: 3;
}

polling filtrate filbtn input:checked + span {
	background-color: #4a8bc2;
	color: #ffffff;
}

polling pollist {
	position: relative;
	display: block;
	float: left;
}

polling pollist {
	position: relative;
	display: block;
	float: left;
}

polling pollist > header {
	position: relative;
	width: 100%;
	height: 72px;
	background-color: #eff0f5;
	box-sizing: border-box;
	border-bottom: 1px solid #e0e0e0;
}

polling pollist > header pandect {
	position: relative;
	display: block;
	float: left;
	width: 100px;
	height: 72px;
}

polling pollist > header pandect p {
	height: 36px;
    line-height: 36px;
    text-align: center;
    font-size: 14px;
    margin-top: 10px;
    margin-bottom: -25px;
}

polling pollist > header pandect p span {
	font-weight: 600;
    width: 32px;
    display: inline-block;
    margin: 0 -4px;
}

polling pollist > header pandect p:first-child span {
	color: #4a8bc2;
}

polling pollist > header pandect p:last-child span {
	color: #fb4041;
}

polling pollist > header checkall {
    position: relative;
    display: block;
    float: left;
    margin-top: 39px;
    margin-left: 10px;
    font-size: 14px;
}

polling pollist > header checkall input[type=checkbox] {
    margin-top: 4px;
    position: relative;
    display: block;
    float: left;
    margin-right: 3px;
}

polling pollist > header polsearch {
    position: absolute;
    display: block;
    top: 0;
    bottom: 0;
    right: 576px;
    width: 310px;
    height: 36px;
    margin: auto 0;
}

polling pollist > header polsearch input {
    width: 260px;
    height: 36px;
    border: none;
    font-size: 16px;
    padding: 0 7px;
}

polling pollist > header polsearch i {
    width: 36px;
    height: 36px;
    position: absolute;
    display: block;
    top: 0;
    right: 0;
    background-image: url(../images/search.jpg);
    background-repeat: no-repeat;
    background-size: 36px 36px;
}

polling pollist > header polbtns {
    height: 100%;
    position: absolute;
    display: block;
    right: 100px;
}

polling pollist > header polbtns div {
    height: 32px;
    position: relative;
    float: left;
    margin-top: 22px;
    margin-left: 42px;
    padding: 0 24px;
    color: #fff;
    line-height: 32px;
    font-size: 14px;
    text-align: center;
    border: 1px solid #aaa;
    box-sizing: border-box;
}

polling pollist > header polbtns div.blue {
	background-color: #7ebc59;
}

polling pollist > header polbtns div.green {
	background-color: #3084d6;
}

polling pollist > header morebtns {
    height: 100%;
    position: absolute;
    display: block;
    right: 0px;
}

polling pollist > header morebtns i {
    width: 36px;
    height: 36px;
    position: absolute;
    display: block;
    top: 20px;
    right: 32px;
    background-image: url(../images/modelManage.png);
    background-repeat: no-repeat;
    background-size: 36px 36px;
    cursor: pointer;
}

polling pollist > header morebtns i:hover {
    background-image: url(../images/modelManageHover.png);
}

polling pollist listwrap {
    position: relative;
    width: 100%;
    display: block;
    margin-bottom: 16px;
}

polling pollist listwrap > header {
    position: relative;
    min-width: 90%;
    margin: 0 auto 16px;
    height: 46px;
    border-bottom: 1px solid #ccc;
    line-height: 50px;
    font-size: 18px;
    color: #3f8ed9;
    display: block;
}

polling pollist listwrap > header label {
    margin-left: 8px;
}

pollist listwrap listarea {
    position: relative;
    display: block;
    clear: both;
    margin: 0 auto;
}

listarea browse {
	display: inline-table;
    height: 152px;
    box-sizing: border-box;
    border: 1px solid #797979;
    margin-left: 8px;
    margin-bottom: 8px;
    position: relative;
    width: 360px;
}

pollist cmslist {
	position: relative;
	width: 100%;
	overflow: hidden;
	display: block;
    left: 0;
    border: none;
    z-index: 1;
    background-color: rgba(255,255,255,0);
}

pollist cmslist cmsscroll {
	position: absolute;
	display: block;
	width: 100%;
}

pollist cmslist cmsscroll:hover + .wrap-scroll-bar {
	opacity: 1;
}

listarea browse > header {
	position: relative;
    height: 30px;
    background-color: rgba(225, 234, 235, 1);
    line-height: 30px;
    color: #444;
    font-size: 15px;
}

listarea browse > header input {
    position: relative;
    /* margin: 8px; */
    float: left;
    opacity: 0;
}

listarea browse > header input:checked + label {
    color: #1d92cc;
    font-weight: 600;
}

listarea browse > header status {
    position: absolute;
    display: block;
    height: 30px;
    line-height: 30px;
    padding: 0 11px;
    font-size: 15px;
    top: 0;
    right: 0;
    background-color: rgba(225, 234, 235, 1);
}

listarea browse > header status.red {
    color: #f13636;
}

listarea browse > header status.blue {
    color: #38aa3f;
}

listarea browse > header status.green {
    color: #6eb2b8;
}

listarea browse bpreview {
    position: relative;
    display: block;
    width: 100%;
    height: 122px;
    background-color: #515860;
    z-index: 7;
}

listarea browse bpreview bcwrap {
    position: absolute;
    display: block;
    width: 100px;
    height: 40px;
    background-color: #000000;
    margin: auto;
    top: 0;
    left: 0;
    bottom: 0;
    right: 0;
    padding: 8px;
}

listarea browse bpreview bcms {
    width: 100%;
    height: 100%;
    position: relative;
    display: block;
    overflow: hidden;
}

listarea browse bpreview pwrap {
    position: absolute;
    display: block;
    width: 100%;
    height: 100%;
    right: -100%;
}

listarea browse bpreview pwrap p {
    position: absolute;
    width: 100%;
    text-align: left;
}

listarea browse bpreview pwrap picon {
    position: absolute;
    display: block;
    height: 100%;
    background-repeat: no-repeat
}

listarea browse cnav {
    position: absolute;
    display: block;
    bottom: 6px;
    height: 8px;
    right: 10px;
    z-index: 8;
}

listarea browse cnav p {
    float: left;
    background-color: #78c8cb;
    width: 8px;
    height: 8px;
    border-radius: 4px;
    overflow: hidden;
    color: #78c8cb;
    margin-left: 4px;
    cursor: pointer;
}

listarea browse bottombar {
	position: absolute;
    display: block;
    width: 100%;
    height: 36px;
    background-color: rgba(122, 129, 136, 1);
    border: 1px solid #797979;
    left: -1px;
    bottom: 0;
    transition: bottom 100ms ease;
}

listarea browse:hover > bottombar {
	bottom: -36px;
    transition: bottom 200ms ease;
}

listarea browse:hover {
	z-index: 2;
}

listarea browse bottombar span {
	position: relative;
	display: block;
	float: right;
	color: #fff;
	font-size: 14px; 
	height: 36px;
    line-height: 36px;
    padding: 0 12px;
    cursor: pointer;
    transition: all 200ms ease;
}

listarea browse bottombar span:hover {
	font-weight: 600;
    transition: all 200ms ease;
}

modelmanage {
    position: absolute;
    display: block;
    width: 1150px;
    border: 1px solid #b6b6b6;
    top: 0px;
    right: 0px;
    z-index: 51;
    background-color: #fff;
}

modelmanage > header {
    position: relative;
    width: 100%;
    height: 34px;
    border-bottom: 1px solid #b6b6b6;
    background-color: #6ccac9;
}

modelmanage span {
    position: relative;
    display: block;
    height: 34px;
    line-height: 34px;
    color: #fff;
    font-size: 18px;
    margin-left: 12px;
}

modelmanage > header i {
    position: absolute;
    display: block;
    width: 34px;
    height: 34px;
    line-height: 34px;
    color: #fff;
    font-size: 28px;
    top: 0;
    right: 0;
    text-align: center;
    cursor: pointer;
    transition: color 240ms ease;
}

modelmanage > header i:hover {
    color: #F91A55;
    transition: color 240ms ease;
}

div.mlistwrap {
    position: absolute;
    width: 100%;
}

div.mlistwrap {
    position: absolute;
    width: 100%;
}

modelview {
    position: absolute;
    display: block;
    width: 1124px;
    height: 122px;
    background-color: #ccc;
    z-index: 109;
    top: -122px;
    left: 13px;
    opacity: 0;
}

modelview > div {
    width: 374.5px;
    float: left;
    opacity: 1 !important;
}

/* 临时 */
.resstyle {
    position: absolute;
    top: 60px;
    right: 0px;
    width: 100px;
    background-color: #fff;
    box-shadow: 0 0 3px #6f6f6f;
    text-align: center;
    font-size: 14px;
    cursor: pointer;
    z-index: 999;
}

@media screen and (max-width: 1366px) {
    polling polaside {
        width: 134px;
    }

    polling filtrate filbtn {
        width: 100%;
    }

