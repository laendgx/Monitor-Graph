@charset "utf-8";
html {
	width: 100%;
	height: 100%;
}
body {
	height: 100%;
}

cmwindow {
	position: relative;
	display: block;
	overflow: hidden;
	margin: 20px;
}

cmwindow .cmap,
cmwindow cmap {
	position: absolute;
	display: block;
	top: 0;
	left: 0;
}

cmwindow cmap {
	z-index: 11;
}

cmwindow cmap div {
	position: absolute;
	width: 32px;
	height: 32px;
	background-color: #eee;
	cursor: pointer;
}

cmwindow eagle {
	display: block;
	position: fixed;
	border: 1px solid #000;
	box-shadow: 0 0 1px #fff;
	top: 20px;
	right: 20px;
	z-index: 21;
	overflow: hidden;
}

cmwindow eagle eawindow {
	display: block;
	position: absolute;
	border-color: rgba(0,0,0,.2);
	border-style: solid;
	border-width: 320px;
	top: -320px;
	left: -320px;
	cursor: pointer;
}

cmwindow bgctrl {
	position: absolute;
	display: block;
	z-index: 41;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
}

cmscompile,
cmslist,
cmsmodel {
	position: absolute;
	display: block;
	width: 590px;
	border: 1px solid #b6b6b6;
	top: 0;
	left: 0;
	z-index: 51;
	background-color: #fff;
}

cmsmodel {
	width: 678px;
	left: 590px;
}

cmscompile > header,
cmslist > header,
cmsmodel > header {
	position: relative;
	width: 100%;
	height: 34px;
	border-bottom: 1px solid #b6b6b6;
	background-color: #6ccac9;
}

cmscompile > header span,
cmslist > header span,
cmsmodel > header span {
	position: relative;
	display: block;
	height: 34px;
	line-height: 34px;
	color: #fff;
	font-size: 18px;
	margin-left: 12px;
}

cmscompile > header i,
cmslist > header i,
cmsmodel > header i {
	position: absolute;
	display: block;
	width: 34px;
	height: 34px;
	line-height: 34px;
	color: #fff;
	font-size: 28px;
	top: 0;
	right: 0;
	text-align: center;
	cursor: pointer;
	transition: color 240ms ease;
}

cmscompile > header i:hover,
cmslist > header i:hover,
cmsmodel > header i:hover {
	color: #F91A55;
	transition: color 240ms ease;
}

cmscompile browse,
cmsedit browse {
	position: relative;
	display: block;
	width: 100%;
	height: 149px;
	border-bottom: 1px solid #b6b6b6;
	background-color: #f3f3f3;
}

cmscompile browse preview,
cmsedit browse preview {
	position: relative;
	display: block;
	width: 566px;
	height: 130px;
	top: 12px;
	margin-left: 12px;
	background-color: #515860;
}

cmscompile browse cwrap,
cmsedit browse cwrap {
	position: absolute;
	display: block;
width: 192px;
height: 96px;
	top: 0;
	left: 0;
	right: 0;
	bottom: 0;
	margin: auto;
	background-color: #000;
	padding: 4px;
}

cmscompile browse cwrap {
	color: #FFFF00;
	text-align: center;
}

cmscompile browse cwrap cms {
	position: relative;
	display: block;
	width: 100%;
	height: 100%;
	top: 0;
	left: 0;
	overflow: hidden;
}

cmscompile browse cwrap cms pwrap {
	position: absolute;
	display: block;
	width: 100%;
	height: 100%;
	top: 0;
	right: -100%;
}

cmscompile > browse cwrap p,
cmsedit browse cwrap p {
	position: absolute;
	width: 100%;
	text-align: left;
}

cmscompile browse cnav {
	position: absolute;
	display: block;
	height: 18px;
    bottom: 12px;
    right: 18px;
	clear: both;
}

cmscompile browse cnav p {
	position: relative;
	display: block;
	float: left;
	width: 18px;
	height: 18px;
	background-color: #000;
	color: #fff;
	font-size: 13px;
	line-height: 18px;
	text-align: center;
	color: #fff;
	cursor: pointer;
}

cmscompile browse cnav p:first-child {
	border-radius: 3px 0 0 3px;
}

cmscompile browse cnav p:last-child {
	border-radius: 0 3px 3px 0;
}

cmscompile > div.tplwrap,
cmsedit > div.tplwrap {
	position: relative;
}

cmscompile info {
	position: relative;
	display: block;
	width: 100%;
	height: 68px;
	border-bottom: 1px solid #b6b6b6;
}

cmscompile info ul {
	position: relative;
	width: 474px;
	padding-top: 4px;
	float: left;
}

cmscompile info ul li {
	position: relative;
	width: 100%;
	height: 20px;
	line-height: 20px;
	text-indent: 12px;
	color: #8f9396;
	font-size: 12px;
}

cmscompile info btns {
	position: relative;
	display: block;
	width: 116px;
	height: 100%;
	float: left;
}

cmscompile info btns p {
    position: relative;
    padding: 0 10px;
    height: 24px;
    line-height: 26px;
    margin-top: 32px;
    margin-left: 16px;
    float: right;
    border: 1px solid #b5becb;
    border-radius: 4px;
    text-align: center;
    font-size: 13px;
    color: #8493a8;
    cursor: pointer;
    margin-right: 20px;
}

cmscompile multitxta btns select {
	position: relative;
    height: 24px;
    margin-top: 4px;
	margin-left: 12px;
	float: left;
	border: 1px solid #8e9bae;
	border-radius: 4px;
	font-size: 14px;
	color: #888888;
	width: 90px;
}

cmscompile multitxta btns select.type1 {
	width: 90px;
}

cmscompile multitxta btns select.type2 {
	width: 46px;
}

cmscompile multitxta btns select.type3 {
	width: 64px;
}

cmscompile multitxta btns i {
	position: relative;
	display: block;
	width: 26px;
    height: 24px;
    margin-top: 4px;
	margin-left: 12px;
	float: left;
	border: 1px solid rgba(142,155,174,0);
	border-radius: 3px;
	background-image: url(../images/textarea.png);
	background-size: 180px 60px;
    background-position-y: -4px;
    box-sizing: border-box;
    cursor: pointer;
}

cmscompile multitxta btns i.increase {
	background-position-x: -2px; 
}

cmscompile multitxta btns i.reduce {
	background-position-x: -32px; 
}

cmscompile multitxta btns i.textleft {
	background-position-x: -62px; 
}

cmscompile multitxta btns i.textcenter {
	background-position-x: -92px; 
}

cmscompile multitxta btns i.textright {
	background-position-x: -122px; 
}

cmscompile multitxta btns i.textcolor {
	background-image: none;
	background-color: #FFFF00;
}

cmscompile multitxta btns i.icon {
	background-position-x: -152px; 
}

cmscompile multitxta btns i.textcolor colors {
    position: absolute;
    display: block;
    bottom: -34px;
    left: -6px;
    height: 26px;
    width: 94px;
    background-color: #fff;
    border: 1px solid #8e9bae;
    border-radius: 3px;
    z-index: 9;
    background: linear-gradient(to top, #e6e6e6, #bebcbb);
}

cmscompile multitxta btns i.textcolor colors i {
	position: relative;
	display: block;
	width: 26px;
    height: 24px;
    margin-top: 1px;
	margin-left: 4px;
	float: left;
    cursor: pointer;
    background-image: none;
}

cmscompile multitxta btns i.textcolor colors i.yellow {
	background-color: #ffff00;
}

cmscompile multitxta btns i.textcolor colors i.red {
	background-color: #ff0000;
}

cmscompile multitxta btns i.textcolor colors i.blue {
	background-color: #00ff00;
}

cmscompile multitxta btns i:hover,
cmscompile multitxta btns input:hover + i{
	background-position-y: -34px; 
}

cmscompile multitxta btns input {
	position: absolute;
	width: 26px;
    height: 24px;
    opacity: 0;
    z-index: 11;
    cursor: pointer;
}

cmscompile multitxta btns input[type=button]:active + i,
cmscompile multitxta btns input[type=radio]:checked + i {
	background-color: #fff;
	border-color: rgba(142,155,174,1);
	background-position-y: -34px; 
}

multitxta {
	position: absolute;
	display: block;
	width: 100%;
	height: 115px;
	border-bottom: 1px solid #b6b6b6;
	bottom: -116px;
}

cmscompile > div.tplwrap multitxta {
	bottom: -185px;
}

multitxta btns {
	position: relative;
	display: block;
	width: 100%;
	height: 34px;
	border-bottom: 1px solid #b6b6b6;
	background: linear-gradient(to top, #e6e6e6, #bebcbb);
}

multitxta textarea {
	position: relative;
	display: block;
	width: 580px;
	height: 70px;
	resize: none;
	overflow-x: hidden;
	overflow-y: auto;
	padding: 5px;
	border: none;
}

cmscompile > footer,
cmslist > footer,
cmsedit > footer {
	position: relative;
	width: 100%;
	height: 60px;
	background-color: #eff0f5;
}

cmscompile > footer,
cmsedit > footer {
	margin-top: 116px;
}

cmscompile > footer btns,
cmsedit > footer btns {
    position: absolute;
    display: block;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    margin: 19px auto 0;
    width: max-content;
    height: max-content;
}

cmscompile > footer btns p,
cmsedit > footer btns p {
	position: relative;
    float: left;
    width: 92px;
    height: 28px;
    border: 1px solid #b5becb;
    border-radius: 3px;
    margin-right: 20px;
    line-height: 28px;
    text-align: center;
    color: #979797;
    font-size: 14px;
    cursor: pointer;
}

cmscompile > footer btns p:last-child,
cmsedit > footer btns p:last-child {
	margin-right: 0px;
}

cmslist {
	left: 592px;
	overflow: hidden;
}

cmslist > footer p {
	position: relative;
	width: 102px;
	height: 28px;
	border: 1px solid #b5becb;
	border-radius: 3px;
	line-height: 28px;
	text-align: center;
	color: #979797;
	font-size: 14px;
	cursor: pointer;
	margin: 0 auto;
    top: 19px;
}

cmslist div.table {
	width: 100%;
	/* min-height: 189px; */
    height: 334px;
	border-bottom: 1px solid #b6b6b6;
	position: relative;
}

cmslist table {
	width: 100%;
	position: absolute;
}

cmslist table:hover + div.wrap-scroll-bar {
	opacity: 1;
}

cmslist table thead {
	width: 100%;
	height: 0;
}

cmslist table tbody td {
	height: 36px;
	border: 1px solid #dddddd;
	border-collapse: collapse;
	text-align: center;
	color: #6b6b6b;
	font-size: 14px;
}

cmsedit div.guide {
	position: relative;
	width: 100%;
	height: 79px;
	border-bottom: 1px solid #b6b6b6;
	background-color: #f7f6f6;
}

cmsedit div.guide input {
	position: absolute;
	width: 250px;
	height: 36px;
	border: 1px solid #b6b6b6;
	background-color: #f7f6f6;
	top: 0;
	left: 0;
	bottom: 0;
	right: 0;
	margin: auto;
	border-radius: 4px;
}

cmsedit div.guide input {
	position: absolute;
	width: 250px;
	height: 36px;
	border: 1px solid #b6b6b6;
	background-color: #f7f6f6;
	top: 0;
	left: 0;
	bottom: 0;
	right: 0;
	margin: auto;
	border-radius: 4px;
	padding: 5px;
}

cmsedit edittpl > header {
	position: relative;
	width: 100%;
	height: 32px;
	line-height: 32px;
	border-bottom: 1px solid #b6b6b6;
	background-color: #eff0f5;
}

cmsedit edittpl {
	position: relative;
	width: 100%;
	display: block;
}

cmsedit edittpl > header p,
cmsedit edittpl > header div{
	position: relative;
	height: 32px;
	line-height: 32px;
	margin-left: 14px;
	float: left;
	font-size: 14px;
	color: #6b6b6b;
}

cmsedit edittpl > header p span {
	position: relative;
	color: #3c8bd6;
	margin-left: 2px;
	margin-right: 7px;
	font-size: 16px;
	font-weight: 600; 
}

cmsedit edittpl > header div {
	margin-right: 14px;
	margin-left: 0;
	float: right;
	color: #3c8bd6;
	cursor: pointer;
}

cmsmodel btns {
	position: relative;
	display: block;
	width: 100%;
	height: 73px;
	background-color: #eff0f5;
	border-bottom: 1px solid #b6b6b6;
}

cmsmodel btns filbtn {
	position: relative;
	display: block;
	float: left;
	width: 114px;
	height: 73px;
}

cmsmodel btns input[type=radio],
cmsmodel btns p {
    width: 104px;
    position: absolute;
    top: 0;
    bottom: 0;
    height: 30px;
    right: 0;
    margin: auto;
}

cmsmodel btns input[type=radio] {
	opacity: 0;
	z-index: 3;
}

cmsmodel btns p {
    background-color: #fff;
    box-sizing: border-box;
    border: 1px solid #cad3df;
    border-radius: 6px;
    color: #8493a8;
    font-size: 14px;
    line-height: 30px;
    text-align: center;
    overflow: hidden;
    transition: all 200ms ease;
    z-index: 2;
}

cmsmodel btns input[type=radio]:hover + p {
	background-color: #eef1f5;
	box-shadow: 0 0 3px #cad3df;
    transition: all 200ms ease;
}

cmsmodel btns input[type=radio]:checked + p {
	color: #fff;
	background-color: #4a8bc2;
	z-index: 3;
}

cmsmodel btns modsearch {
	position: relative;
	display: block;
	height: 73px;
	width: 210px;
	float: left;
}

cmsmodel btns modsearch i {
	position: absolute;
    height: 34px;
    width: 34px;
    top: 0;
    bottom: 0;
    margin: auto;
    background-color: #000;
    left: 164px;
}

cmsmodel btns modsearch input {
	position: absolute;
	border: 1px solid #ddd;
	height: 34px;
	width: 144px;
	top: 0;
	bottom: 0;
	margin: auto;
	left: 10px;
	padding: 0 4px;
}

cmsmodel modellist {
	position: relative;
	display: block;
	width: 100%;
	height: 600px;
}

cmsmodel modellist header {
	position: relative;
	width: 100%;
	height: 52px;
	line-height: 56px;
	color: #3085d6;
	font-size: 15px;
	text-indent: 12px;
}

cmsmodel modellist div.table {
	position: relative;
	width: 660px;
	height: 534px;
	margin: 0 auto 15px;
}

cmsmodel modellist div.table table {
	position: absolute;
	width: 100%;
}

cmsmodel modellist div.table table tbody tr:nth-child(odd) {
	background-color: #f9f9f9;
}

cmsmodel modellist div.table table tr:hover {
	background-color: #d2edec;
}

cmsmodel modellist div.table table thead {
	font-size: 16px;
}

cmsmodel modellist div.table table tbody {
	font-size: 14px;
}

cmsmodel modellist div.table table th,
cmsmodel modellist div.table table td {
	border: 1px solid #dddddd;
	border-collapse: collapse;
	height: 42px;
	text-align: center;
}

cmsmodel modellist div.table table th:nth-child(2),
cmsmodel modellist div.table table td:nth-child(2) {
	text-align: left;
	text-indent: 65px;
}






div.devices {
	position: absolute;
	height: 20px;
	bottom: 0;
	left: 0;
	z-index: 99;
}
div.devices div {
	float: left;
	height: 20px;
	width: 30px;
	border: 1px solid #fff;
	margin-left: 1px;
}