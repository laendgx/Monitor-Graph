<%@ page pageEncoding="UTF-8"%>
<cmsmodel ng-show='modelList' ng-hide='!modelList'>
	<header>
		<span>情报板</span>
		<i ng-click='modelList=false'>×</i>
	</header>
	<btns>
		<filbtn ng-repeat='type in modelType'>
			<input type='radio' name='model'>
			<p>{{type.name}}</p>
		</filbtn>
		<modsearch>
			<input type="text">
			<i></i>
		</modsearch>
	</btns>
	<modellist>
		<header>天气提醒</header>
		<div class="table">
			<table>
				<thead>
					<tr>
						<th>序号</th>
						<th>节目单内容</th>
						<th>操作</th>
					</tr>
				</thead>
				<tbody>
					<tr>
						<td>1</td>
						<td>1</td>
						<td>使用</td>
					</tr>
				</tbody>
			</table>
		</div>
	</modellist>
</cmsmodel>